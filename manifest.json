{
  "name": "Chrome History Selector",
  "version": "1.0",
  "description": "Select All history matching the given criterion",
  "manifest_version": 2,
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "hello.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "sayHello.js"
    ]
  }
}